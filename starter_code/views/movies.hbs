<h1>{{title}}</h1>

<h3>Click on the movies to check the showtimes!</h3>

<div class="movie-list">
	{{#each movieList}}
	<div class="one-movie">
		<img src="{{this.image}}" alt="{{this.title}} poster">
		<h2>{{this.title}}</h2>

		<a href="/movie/{{this._id}}">See more</a>
	</div>
	{{/each}}
	
</div>